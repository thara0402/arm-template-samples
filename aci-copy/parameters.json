{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceNamePrefix": {
            "value": "gooner1017"
        },
        "imageType": {
            "value": "Public"
        },
        "imageName": {
            "value": "thara0402/cypress-docker:1.3.0"

            "value": "1.5"
        },
        "restartPolicy": {
            "value": "Never"
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "80",
                    "protocol": "TCP"
                }
            ]
        },
        "environmentVariables": {
            "value": [
                {
                    "name": "cypress_target_url",
                    "value": "https://www.google.co.jp"
                }
            ]
        },
        "numberOfInstances": {
            "value": 3
        }
    }
